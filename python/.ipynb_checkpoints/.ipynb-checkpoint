    "def s(p):\n",
    "    output = 0\n",
    "    for i in range(1, 6):\n",
    "        output += 1\n",
    "        output *= (p-i)\n",
    "    for i in range(2, p-5):\n",
    "        output *= i\n",
    "        output = output%p\n",
    "    output = output%p\n",
    "    return output"
